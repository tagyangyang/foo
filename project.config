[project]
	description = my foo project
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/tagyangyang
[access "refs/for/refs/*"]
	read = group user/tagyangyang
	push = group user/tagyangyang
	label-Code-Review = -2..+2 group user/tagyangyang
	label-Verified = -1..+1 group user/tagyangyang
	rebase = group user/tagyangyang
	removeReviewer = group user/tagyangyang
	submit = group user/tagyangyang
[access "refs/heads/*"]
	read = group user/tagyangyang
	create = group user/tagyangyang
	push = +force group user/tagyangyang
	pushMerge = group user/tagyangyang
[access "refs/tags/*"]
	push = +force group user/tagyangyang
	pushTag = group user/tagyangyang
	pushSignedTag = group user/tagyangyang
